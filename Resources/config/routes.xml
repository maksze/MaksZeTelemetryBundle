<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://symfony.com/schema/routing
  https://symfony.com/schema/routing/routing-1.0.xsd"
>
  <route
    id="maksze_telemetry.yandex_webmaster_controller"
    controller="maksze_telemetry.webmaster_controller::yandex"
    path="/yandex_{id}.html"
  ></route>

  <route
    id="maksze_telemetry.google_search_console_controller"
    controller="maksze_telemetry.webmaster_controller::google"
    path="/google{id}.html"
  ></route>
</routes>
